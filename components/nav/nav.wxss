.nav-wrap {
    position: fixed;
    top: 0;
    left: 0;
    width: 750rpx;
    z-index: 99999;
}

.content {
    position: relative;
    width: 100%;
    height: 88rpx;
    display: flex;
    align-items: center;
}

.back {
    position: absolute;
    left: 0;
    top: 0;
    width: 80rpx;
    height: 88rpx;
    z-index: 99999;
}

.back .bord {
    content: ' ';
    position: absolute;
    border-left: 2px solid #000;
    border-top: 2px solid #000;
    width: 24rpx;
    height: 24rpx;
    transform: translate(100%, 125%) rotate(-45deg);
}

.title {
    text-align: center;
    font-weight: 900;
    font-size: 32rpx;
    margin: auto;
}

.home {
    width: 65rpx;
    height: 65rpx;
    position: absolute;
    left: 70rpx;
    top: 16rpx;
}